{
  "expo": {
    "name": "Cherrymobile_v2",
    "slug": "cherrymobile_v2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app_config/cherry-app-icon.png",
    "scheme": "cherryV2",
    "userInterfaceStyle": "automatic",
    "developmentClient": {
      "silentLaunch": true
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cherrymobile.v2"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app_config/cherry-android-adaptive-icon.png",
        "backgroundColor": "#ff0000"
      },
      "splash": {
        "image": "./assets/images/app_config/cherry-android-adaptive-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#ff0000"
      },
      "package": "com.cherrymobile.v2",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/app_config/cherry-app-icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
